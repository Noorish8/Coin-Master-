<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    tools:context=".Guide"
    android:background="#192D55">

    <ImageView
        android:id="@+id/back"
        android:layout_width="99dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"

        android:layout_alignParentRight="true"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="312dp"
        android:layout_marginRight="312dp"
        android:layout_marginBottom="380dp"
        android:src="@drawable/back"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.072"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.037" />

    <ImageView
        android:id="@+id/imageViewcoinmaster"
        android:layout_width="139dp"
        android:layout_height="164dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="130dp"
        android:layout_marginLeft="130dp"
        android:layout_marginTop="-12dp"
        android:layout_marginEnd="142dp"
        android:layout_marginRight="142dp"
        android:src="@drawable/asset73" />

    <RelativeLayout
        android:layout_width="393dp"
        android:layout_height="531dp"
        android:layout_below="@+id/imageViewcoinmaster"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="39dp"
        android:layout_marginEnd="18dp"
        android:layout_marginRight="18dp"
        android:layout_marginBottom="9dp"
        tools:layout_editor_absoluteX="-16dp"
        tools:layout_editor_absoluteY="-70dp">

        <ImageView
            android:id="@+id/imageViewyellow"
            android:layout_width="@dimen/_250sdp"
            android:layout_height="@dimen/_40sdp"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="@dimen/_31sdp"
            android:src="@drawable/imgpsh_fullsize_animyellow" />

        <ImageView
            android:id="@+id/imageViewciyan"
            android:layout_width="@dimen/_250sdp"
            android:layout_height="@dimen/_40sdp"
            android:layout_below="@+id/imageViewyellow"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="@dimen/_31sdp"
            android:layout_marginTop="2dp"
            android:src="@drawable/imgpsh_fullsize_animciyan" />

        <ImageView
            android:id="@+id/imageViewred"
            android:layout_width="@dimen/_250sdp"
            android:layout_height="@dimen/_40sdp"
            android:layout_below="@+id/imageViewciyan"
            android:layout_centerInParent="true"
            android:layout_marginLeft="@dimen/_31sdp"
            android:layout_marginTop="4dp"
            android:src="@drawable/imgpsh_fullsize_animred" />

        <ImageView
            android:id="@+id/imageViewperpol"
            android:layout_width="@dimen/_250sdp"
            android:layout_height="@dimen/_40sdp"
            android:layout_below="@+id/imageViewred"
            android:layout_centerInParent="true"
            android:layout_marginLeft="@dimen/_31sdp"
            android:layout_marginTop="3dp"
            android:src="@drawable/imgpsh_fullsize_animperpol" />

        <ImageView
            android:id="@+id/imageVieworange"
            android:layout_width="@dimen/_250sdp"
            android:layout_height="@dimen/_40sdp"
            android:layout_below="@+id/imageViewperpol"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="@dimen/_31sdp"
            android:layout_marginTop="1dp"
            android:src="@drawable/imgpsh_fullsize_animorange" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="@dimen/_259sdp"
            android:layout_height="@dimen/_40sdp"
            android:layout_alignBottom="@+id/imageViewyellow"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginStart="61dp"
            android:layout_marginLeft="61dp"
            android:layout_marginTop="6dp"
            android:layout_marginEnd="14dp"
            android:layout_marginRight="14dp"
            android:layout_marginBottom="0dp"
            android:text="1.Play it and Grab the bonus"
            android:textColor="@color/white"
            android:textSize="@dimen/_13sdp" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="@dimen/_259sdp"
            android:layout_height="@dimen/_40sdp"
            android:layout_above="@+id/textView3"
            android:layout_below="@+id/textView1"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginStart="57dp"
            android:layout_marginLeft="57dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="18dp"
            android:layout_marginRight="18dp"
            android:layout_marginBottom="13dp"
            android:text="2.Learn slot machine Machanism"
            android:textColor="@color/white"
            android:textSize="@dimen/_13sdp" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="29dp"
            android:layout_above="@+id/textView4"
            android:layout_below="@+id/imageViewciyan"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginStart="63dp"
            android:layout_marginLeft="63dp"
            android:layout_marginTop="17dp"
            android:layout_marginEnd="12dp"
            android:layout_marginRight="12dp"
            android:layout_marginBottom="11dp"
            android:text="3.Finish all events"
            android:textColor="@color/white"
            android:textSize="@dimen/_13sdp" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="53dp"
            android:layout_above="@+id/textView5"

            android:layout_below="@+id/imageViewred"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginStart="55dp"
            android:layout_marginLeft="55dp"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="20dp"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="11dp"
            android:text="4.Concentrate on coin accordingly"
            android:textColor="@color/white"
            android:textSize="@dimen/_13sdp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="38dp"
            android:layout_below="@+id/imageViewperpol"

            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginStart="59dp"
            android:layout_marginLeft="59dp"
            android:layout_marginTop="14dp"
            android:layout_marginEnd="17dp"
            android:layout_marginRight="17dp"
            android:text="5.Collect your card"
            android:textColor="@color/white"
            android:textSize="@dimen/_13sdp" />
    </RelativeLayout>


</RelativeLayout>